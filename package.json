{
  "name": "diggaihh-medtech-saas",
  "version": "1.0.0",
  "description": "DiggAiHH - Medical Technology SaaS Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test:unit": "node --test src/utils/seo.test.js",
    "test:frontend": "vitest",
    "test:frontend:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:server": "npm --prefix server test",
    "test:infra": "node tests/infra/docker-smoke.mjs",
    "test:all": "npm run test:unit && npm run test:frontend:run && npm run test:server && npm run test:e2e && npm run test:infra",
    "security:audit": "npm audit --audit-level=moderate",
    "security:zap": "bash scripts/zap-baseline.sh"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "27.4.0",
    "postcss": "^8.4.32",
    "rollup-plugin-visualizer": "6.0.5",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "vite": "^5.0.8",
    "vitest": "4.0.16"
  },
  "overrides": {
    "esbuild": "0.27.2"
  }
}
